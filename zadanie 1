#include <iostream>
 using namespace std;
  int main(){
  setlocale(LC_ALL, "russia");

  int n;

  cout << "Введите количество чисел, которое вы хотите ввести: ";
  cin >> n;

  cout << "Введите " << n << " чисел: "; 
                                        
  int *dinamich_array = (int*)malloc(n * sizeof(int));
                                        // динамический массив
  for (int i = 0; i < n; i++) { 
    cin >> dinamich_array[i];  // считываем числа в ячейки массива
  }
  int i, j ;{
}
  for (int i = 0; i < 10; i++) {
    cout<<i;
  }
    for (int j = 0; j < 9; j++) {
      cout<<j;
    }
    {
      if (dinamich_array[j] > dinamich_array[j + 1]) {
        int b = dinamich_array[j]; 
        dinamich_array[j] = dinamich_array[j + 1]; 
        dinamich_array[j + 1] = b; 
      }
    }

  return 0;
}
